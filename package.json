{
  "name": "ai-ide",
  "version": "0.9.7.5",
  "private": true,
  "author": "notbadai",
  "license": "ISC",
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "@types/express": "^5.0.3",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.5",
    "browserify": "^17.0.0",
    "electron": "^36.4.0",
    "electron-packager": "^17.1.2",
    "http-server": "^14.1.1",
    "sass": "latest",
    "tsify": "^5.0.4",
    "typescript": "latest",
    "uglify-js": "^3.12.8",
    "uglifyify": "^5.0.2",
    "watchify": "^4.0.0"
  },
  "main": "dist/platform/main.js",
  "scripts": {
    "build": "$npm_execpath run build:ui && $npm_execpath run build:sass",
    "watch": "$npm_execpath run watch:ui & $npm_execpath run watch:sass & $npm_execpath run serve",
    "serve": "http-server dist",
    "build:ui": "mkdir -p dist/js && browserify ui/src/main.ts -t uglifyify -d -p [ tsify -p tsconfig.json] | uglifyjs -cm -o dist/js/bundle.min.js --source-map \"content=inline,url=bundle.min.js.map,includeSources\"",
    "watch:ui": "mkdir -p dist/js && watchify ui/src/main.ts -t uglifyify -d -p [ tsify -p tsconfig.json] -v -o dist/js/bundle.min.js",
    "build:sass": "sass --style compressed ui/styles/style.scss:dist/css/style.css",
    "watch:sass": "sass --style compressed --watch ui/styles/style.scss:dist/css/style.css",
    "build:electron": "tsc",
    "electron": "npm run build:electron && electron .",
    "electron:dev": "npm run build:electron && (npm run watch:ui & npm run watch:sass & electron .)",
    "package:electron": "npm run build:electron && DEBUG=electron-packager electron-packager . \"ai-ide-v$npm_package_version\" --out=dist/desktop --overwrite --asar.unpack='**/*.node' --app-version=$npm_package_version",
    "postinstall": "electron-rebuild -f -w node-pty"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.27.1",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lang-markdown": "^6.3.2",
    "@codemirror/lang-python": "^6.1.5",
    "@codemirror/lang-sql": "^6.9.1",
    "@codemirror/lang-yaml": "^6.1.2",
    "@codemirror/state": "^6.4.1",
    "@codemirror/view": "^6.26.1",
    "@replit/codemirror-indentation-markers": "^6.5.3",
    "@replit/codemirror-vscode-keymap": "^6.0.2",
    "@uiw/codemirror-theme-vscode": "^4.23.12",
    "@xterm/addon-fit": "^0.10.0",
    "chokidar": "^3.6.0",
    "codemirror": "^6.0.1",
    "diff": "^8.0.2",
    "diff-match-patch": "^1.0.5",
    "electron-store": "^8.2.0",
    "express": "^4.19.2",
    "highlight.js": "^11.11.1",
    "ignore": "^7.0.5",
    "js-yaml": "^4.1.0",
    "marked": "^4.3.0",
    "node-pty": "^0.10.1",
    "simple-git": "^3.28.0",
    "uuid": "^11.1.0",
    "xterm": "^5.3.0"
  }
}
